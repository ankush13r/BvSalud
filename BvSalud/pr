    "title": "Evaluación retrospectiva de la eficacia de la bumetanida intravenosa y comparación de la potencia con furosemida",
    "db": "IBECS",
    "pmid": "ibc-111120",
    "meshMajor": [
        "Humans",
        "Heart Failure therapy",
        "Female",
        "Furosemide",
        "Injections, Intravenous",
        "Furosemide use",
        "Injections, Intravenous & administration",
        "Retrospective Studies",
        "Therapeutic Equivalency",
        "Male",
        "Heart Failure & control"
