    "journal": "Rev. colomb. anestesiol",
    "title": null,
    "db": "LILACS",
    "pmid": "lil-218082",
    "meshMajor": [
        "Humans",
        "Anesthesia, Intravenous",
        "Anesthesia, Intravenous effects",
        "Alfentanil effects",
        "Alfentanil",
        "Propofol effects",
        "Propofol"
