    "db": "IBECS",
    "pmid": "ibc-162110",
    "meshMajor": [
        "Case-Control Studies",
        "Humans",
        "Hypertension",
        "Vitamin D Deficiency",
        "Risk Factors",
        "Eating",
        "Iran",
        "Depression",
        "Body Weights and Measures & numerical data",
